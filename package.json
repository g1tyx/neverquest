{
  "name": "neverquest",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.1",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "recoil": "^0.6.1",
    "typescript": "^4.6.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
